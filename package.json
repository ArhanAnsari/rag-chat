{
  "name": "@upstash/rag-chat",
  "version": "0.0.41-alpha",
  "main": "./dist/base/index.mjs",
  "types": "./dist/base/index.d.ts",
  "scripts": {
    "test": "",
    "fmt": "prettier --write .",
    "lint": "tsc && eslint \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "build": "bun run build:base && bun run build:react && bun run build:nextjs",
    "build:base": "tsup index.ts --format cjs,esm --dts --outDir dist/base/",
    "build:react": "tsup react.ts --format cjs,esm --dts --outDir dist/react/",
    "build:nextjs": "tsup nextjs.ts --format cjs,esm --dts --outDir dist/nextjs/",
    "prepare": "husky install",
    "a": "yarn build && yarn pack ./dist"
  },
  "bugs": {
    "url": "https://github.com/upstash/rag-chat/issues"
  },
  "description": "Simple RAG Chat using Upstash",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/base/index.d.ts",
      "import": "./dist/base/index.mjs",
      "require": "./dist/base/index.js"
    },
    "./react": {
      "types": "./dist/react/react.d.ts",
      "import": "./dist/react/react.mjs",
      "require": "./dist/react/react.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs/nextjs.d.ts",
      "import": "./dist/nextjs/nextjs.mjs",
      "require": "./dist/nextjs/nextjs.js"
    }
  },
  "homepage": "https://upstash.com/vector",
  "keywords": [
    "vector",
    "upstash",
    "db",
    "rag-chat",
    "ai"
  ],
  "author": "Oguzhan Olguncu <oguzhan@upstash.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/upstash/rag-chat"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "bun-types": "latest",
    "eslint": "^8",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.0.10",
    "prettier": "^3.2.5",
    "tsup": "latest",
    "typescript": "^5.4.5",
    "vitest": "latest"
  },
  "dependencies": {
    "@langchain/community": "^0.2.13",
    "@langchain/core": "^0.2.9",
    "ai": "^3.1.1",
    "cheerio": "^1.0.0-rc.12",
    "d3-dsv": "^3.0.1",
    "html-to-text": "^9.0.5",
    "langchain": "^0.2.0",
    "nanoid": "^5.0.7",
    "pdf-parse": "^1.1.1"
  },
  "peerDependencies": {
    "@upstash/redis": "^1.31.3",
    "@upstash/vector": "^1.1.3",
    "@upstash/ratelimit": "^1.1.3",
    "@langchain/openai": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
